{% if settings.add1 != "" or settings.add2 != "" %}
<li class="ftcon-li">
	{% if settings.add_icon != "" %}
	<span class="con-icon"><i class="{{ settings.add_icon }}"></i></span>
	{% endif %} 
	<span class="con-add">
		{% if settings.add1 != "" %}
		<span>{{ settings.add1 }}</span>
		{% endif %}
		{% if settings.add2 != "" %}
		<span>{{ settings.add2 }}</span>
		{% endif %}
	</span>
</li class="ftcon-li">
{% endif %}
{% if settings.contact != "" %}
<li class="ftcon-li">
	{% if settings.con_icon != "" %}
	<span class="con-icon"><i class="{{ settings.con_icon }}"></i></span>
	{% endif %}
	<span class="con-add">
		{{ settings.contact1 }}<br>
		{{ settings.contact2 }}
	</span>
</li class="ftcon-li">
{% endif %}
{% if settings.email != "" %}
<li class="ftcon-li">
	{% if settings.mail_icon != "" %}
	<span class="con-icon"><i class="{{ settings.mail_icon }}"></i></span>
	{% endif %}
	<span class="con-add">
		<a href="mailto:{{ settings.email1 }}">{{ settings.email1 }}</a><br>
	<a href="mailto:{{ settings.email2 }}">{{ settings.email2 }}</a>
	</span>
</li class="ftcon-li">
{% endif %}
