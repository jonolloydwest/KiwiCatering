  {%- endif -%}
  {% section 'copy-right' %}
  {% include 'back-to-top' %}
  <!-- Necessary JS -->
  {% include 'footer-js' %}
  {%- render 'bold-currency-converter' -%}


{{ "//cdn.shopify.com/s/files/1/0633/1672/1913/t/1/assets/splide.min.js" | script_tag}}

