 <div class="footer_button">
    <div class="container">
      <div class="row">
          <div class="col-md-8">
            <div class="section-title">
            <h2>
                ANY QUESTIONS?
            </h2>
            </div>
</div>
          <div class="col-md-4" style="text-align:right;">
             <a class="btn btn-style-medium" href="tel:0800 374 699">Call Now</a>
          </div>
      </div>
    </div>
</div>  
<section class="footer-top-area" {% if section.settings.footer_bg_img_enable %}style="background-image: url('{% if section.settings.footer_bg_img %}{{ section.settings.footer_bg_img | image_url: width: 2048 }}{% else %}{{ 'placeholder-image.svg' | asset_url }}{% endif %}');"{% endif %}>
   
<div class="container">
    <div class="row">
      <div class="col-xl-12 col-lg-12 col-md-12 col-12 col-center">
        <div class="footer-list-wrap">
          <ul class="footer-list">
            <li class="ftlink-li footer-info">
              {%- render 'company-logo-footer' -%}
              <p class="about-compnay">{{ settings.company_details }}</p>
              {% comment %} {%- render 'social-midea' -%} {% endcomment %}
            </li>


 {% for block in section.blocks %}
              
            <li class="ftlink-li linklist">

             <h6 class="footer-title">{{ block.settings.footer_title1 }}</h6>


              <button class="accordion">{{ block.settings.footer_title1 }}</button>
              <ul class="footer-sublist penal">
                {% assign footer_menu = block.settings.footer_menu_handle1 %}
                {% for link in linklists[footer_menu].links %}
               
            
             {% if linklists[footer_menu].links != '' %}
                <li class="ftsublink-li">
                  <a href="{{ link.url }}" class="ft-sublink">{{ link.title }}</a>
                </li>
                {% endif %}




                {% endfor %}
              </ul>
            </li>
           
            {% endfor %}

            
            {% for block in section.blocks %}
              
            <li class="ftlink-li linklist">

            <h6  class="footer-title">{{ block.settings.footer_title }}</h6>
              <button class="accordion">{{ block.settings.footer_title }}</button>
              <ul class="footer-sublist penal">
                {% assign footer_menu = block.settings.footer_menu_handle %}
                {% for link in linklists[footer_menu].links %}
                {% if linklists[footer_menu].links != '' %}
                <li class="ftsublink-li">
                  <a href="{{ link.url }}" class="ft-sublink">{{ link.title }}</a>
                </li>
                {% endif %}




                {% endfor %}
              </ul>
            </li>
            <li class="ftlink-li newsletter">
              
             
              <h6 class="footer-title">{{ section.settings.newsletter_title }}</h6>
 <p class="Address">
                       <a href="">Click & Collect from 41E Morrin Road, Saint Johns, Auckland. </a>
               </p>
              <div class="news-content">
                {% form 'customer' %}
                {{ form.errors | default_errors }}
                {% if form.posted_successfully? %}
                <p class="text-success">
                  <span>{{ settings.newsletter_success_text }}</span>
                  <span>{{ settings.newsletter_coupan_code }}</span>
                </p>
                {% endif %}
                <input type="hidden" name="contact[tags]" value="newsletter">
                <div class="subscribe-block">
                  <input type="email" name="contact[email]" class="email mail" id="E-mail" value="{% if customer %}{{ customer.email }}{% endif %}" placeholder="{{ settings.email_placeholder }}" autocapitalize="off" required>
                  <div class="email-submit">
                    <button type="submit" class="btn btn-style-medium" name="commit" id="Subscribe">
                      {{ settings.email_button }}
                    </button>
                  </div>
                </div>
                {% endform %}
              </div>
              {%- render 'social-midea' -%}
              {%- render 'payment-icon' -%}
               
            </li>
            {% endfor %}
          </ul>
        </div>
      </div>
    </div>
  </div>
</section>
